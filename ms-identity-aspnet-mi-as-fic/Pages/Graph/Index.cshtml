@page
@model MiFicExamples.Pages.Graph.IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>@Model.CurrentUser.Name Learn about <a href="https://docs.microsoft.com/en-us/azure/app-service/">building Web apps on Azure App Service</a>.</p>

    <table class="table table-striped table-condensed" style="font-family: monospace">
        <tr>
            <th>Property</th>
            <th>Value</th>
        </tr>
        <tr>
            <td>Company Name</td>
            <td>@Model.CurrentUser.CompanyName</td>
        </tr>
        <tr>
            <td>Job Title</td>
            <td>@Model.CurrentUser.JobTitle</td>
        </tr>
        <tr>
            <td>City</td>
            <td>@Model.CurrentUser.City</td>
        </tr>
    </table>
</div>